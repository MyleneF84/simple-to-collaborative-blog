<%= simple_form_for [:authorspace, @article] do |f| %>
  <%= f.input :title %>
  <%= f.input :content %>
  <%= f.input :tag_list, as: :check_boxes, collection: ["Ruby", "Rails", "Front-end", "Back-end"] %>
  <%= f.input :group_id, collection: Group.all, label: "Team" %>
  <%= f.submit "Create", class: "btn btn-primary" %>
<% end %>
