<%= simple_form_for [namespace, @article] do |f| %>
  <%= f.input :title %>
  <%= f.rich_text_area :rich_content %>
  <%= f.input :photo, as: :file %>
  <%= f.input :tag_list, as: :check_boxes, collection: ["Ruby", "Rails", "Front-end", "Back-end"] %>
  <%= f.submit "Update", class:"btn btn-primary" %>
<% end %>
